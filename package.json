{
  "name": "MainProject",
  "version": "1.0.0",
  "description": "The monolithic version of all the microservices, only for development",
  "main": "server.js",
  "license": "MIT",
  "scripts": {
    "eslint": "eslint ./app/ --ext .js", 
    "build": "babel server.js  test/*.spec.js -d dist",
    "test": "npm run build && npm run eslint && lab -T node_modules/lab-babel -t 100 -v -l --coverage-exclude node_modules --coverage-exclude dist",
